#include <string>
#include <vector>
#include <map>
#include <algorithm>
#include <iostream>

using namespace std;

vector<string> solution(vector<string> players, vector<string> callings) {
    vector<string> answer;
    map<string, int> hash;
    int idx;
    
    for(int i=0; i<players.size(); i++){
        hash.insert({players[i], i});
    }
    
    
    for(int i=0; i<callings.size(); i++){
        idx=hash[callings[i]];
        hash[players[idx]]=idx-1;
        hash[players[idx-1]]=idx;
        swap(players[idx], players[idx-1]);
    }
    answer=players;
    return answer;
}
