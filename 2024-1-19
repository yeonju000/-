#include <string>
#include <vector>
#include <iostream>

using namespace std;

int solution(int number, int limit, int power) {
    int answer = 0;
    //기사 수만큼 반복
    for(int i=1; i<number+1; i++){
        int count = 0;
        for(int j=1; j<i+1; j++){
            if(i%j==0){
                count+=1;
            }
        }
        if(count<=limit)
            answer+=count;
        else
            answer+=power;
    }
    return answer;
}
