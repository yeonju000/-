#include <string>
#include <vector>

using namespace std;

//오름차순 함수
    bool isSorted(vector<int> cards1Index, vector<int> cards1Index) {
        for (int i = 1; i < 10; ++i) {
            if ((cards1Index[i - 1] > cards1Index[i]))||(cards2Index[i - 1] > cards2Index[i]) {
                return false;
            }
        }
        return true;
    }

string solution(vector<string> cards1, vector<string> cards2, vector<string> goal) {
    string answer = "";
    vector<int> cards1Index;
    vector<int> cards2Index;
    
    for(int i=0; i<goal.size(); i++) {
        for(int j=0; j<10; j++) {
            if(goal[i]==cards1[j]) {
                cards1Index.push_back(j);
            }
            if(goal[i]==cards2[j]) {
                cards2Index.push_back(j);
            }
        }
    }
    
    if(isSorted(cards1Index, cards2Index)) {
        answer = "Yes";
    }
    else {
        answer = "No";
    }
    return answer;
}
