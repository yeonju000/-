#include <string>
#include <vector>
#include <map>

using namespace std;

vector<int> solution(string s) {
    vector<int> answer;
    map<char, int> table;
    for(int i=0; i<s.length(); i++){
        if(table.find(s[i])==table.end()){
            table.insert({s[i],i});
            answer.push_back(-1);
        }
        else{
            answer.push_back(i-table[s[i]]);
            table[s[i]]=i;
        }
    }
    return answer;
}
