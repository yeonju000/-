#include <string>
#include <vector>
#include <queue>

using namespace std;

vector<int> solution(int k, vector<int> score) {
    vector<int> answer;
    priority_queue<int, vector<int>,greater<int>> minque;
    for(int i=0; i<score.size(); i++){
        if(i<k){
            minque.push(score[i]);
            answer.push_back(minque.top());
            continue;
        }
        if(minque.top()<score[i]){
            minque.pop();
            minque.push(score[i]);
        }
        answer.push_back(minque.top());
    }
    return answer;
}
