#include <string>
#include <vector>
#include <iostream>
#include <numeric>

using namespace std;

int solution(int n, int m, vector<int> section) {
    int answer = 0;
    vector<int> wall(n);
    //색칠해야하는 부분 1로 표시
    for(int i=0; i<section.size(); i++){
        wall[section[i]-1]=1;
    }
    
    //벡터의 합이 0이 되면 종료하기 위한 변수
    int sum=accumulate(wall.begin(), wall.end(),0);
    int check=0;
    int i=0;
    //모두 페인트칠 될 때까지 반복
    while(sum!=0){
        //롤러의 첫부분을 페인트칠이 필요한 벽으로 이동
        while(wall[i]!=1){
            i++;
            //롤러의 끝부분이 벽을 넘는다면 위치 조정
            if(i+m >= n){
                i=n-m;
                break;
            }
        }
        //롤러의 크기만큼 페인트칠
        for(int j=0; j<m; j++){
            if(wall[i+j]==1){
                check=1;
                wall[i+j]=0;
            }
        }
        //페인트칠을 했는지 검사
        if(check==1)
            answer++;
        check=0;
        sum = accumulate(wall.begin(), wall.end(), 0);
    }
    
    return answer;
}
